<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import classes.PaneFrame?>
<?import javafx.geometry.Insets?>

<PaneFrame xmlns="http://javafx.com/javafx"
           xmlns:fx="http://javafx.com/fxml"
           fx:controller="controllers.ProductController"
           prefHeight="400.0" prefWidth="600.0"
           style="-fx-background-color: #bcbcbc;"
           fx:id="paneFrame">
    <top>
        <VBox alignment="CENTER" prefHeight="78.0" prefWidth="400.0" BorderPane.alignment="CENTER">
            <Label fx:id="titleLabel" style="-fx-font-size: 30;"/>
        </VBox>
    </top>
    <center>
        <TableView fx:id="tableView" BorderPane.alignment="CENTER" prefHeight="200.0" prefWidth="300.0">
            <BorderPane.margin>
                <Insets left="25.0" right="25.0"/>
            </BorderPane.margin>
        </TableView>
    </center>
</PaneFrame>