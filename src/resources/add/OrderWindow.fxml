<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import classes.PaneFrame?>
<PaneFrame xmlns="http://javafx.com/javafx"
           xmlns:fx="http://javafx.com/fxml"
           fx:controller="controllers.OrderController"
           prefHeight="400.0" prefWidth="600.0"
           style="-fx-background-color: #bcbcbc;"
           fx:id="paneFrame">
    <top>
        <VBox alignment="CENTER" prefHeight="78.0" prefWidth="400.0" BorderPane.alignment="CENTER">
            <Label fx:id="titleLabel" text="Add Order" style="-fx-font-size: 30;"/>
        </VBox>
    </top>
    <center>
        <Pane>
            <TextField fx:id="fNameTxtFld" layoutX="194.0" prefWidth="75"/>
            <TextField fx:id="lNameTxtFld" layoutX="194.0" layoutY="30.0" prefWidth="75"/>
            <ComboBox fx:id="productNoComboBox" promptText="Pno" layoutX="194.0" layoutY="60.0" prefWidth="75"/>
            <TextField fx:id="quantityTxtFld" layoutX="194.0" layoutY="90.0" prefWidth="75"/>
            <Label layoutX="103.0" layoutY="4.0" text="First Name: "/>
            <Label layoutX="103.0" layoutY="34.0" text="Last Name: "/>
            <Label layoutX="103.0" layoutY="64.0" text="Product No.: "/>
            <Label layoutX="103.0" layoutY="94.0" text="Quantity: "/>
        </Pane>
    </center>
</PaneFrame>
